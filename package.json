{
	"name": "guabao",
	"displayName": "Guabao",
	"description": "Frontend for Guabao",
	"publisher": "scmlab",
	"version": "0.1.7",
	"repository": "https://github.com/scmlab/gcl-vscode",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:guabao"
	],
	"main": "./dist/app.bundle.js",
	"scripts": {
		"clean": "npx bsb -clean-world",
		"build": "npx bsb -make-world && npx lessc style/style.less dist/style.css && webpack --mode development",
		"dev": "webpack --mode development --watch & npx less-watch-compiler style/ dist/ & npx bsb -w",
		"vscode:prepublish": "npx bsb -make-world && npx lessc style/style.less dist/style.css && webpack --mode production"
	},
	"devDependencies": {
		"copy-webpack-plugin": "^6.0.3",
		"less": "^4.1.1",
		"less-loader": "^7.0.2",
		"less-watch-compiler": "^1.14.6",
		"react": "16.9.0",
		"react-dom": "16.9.0",
		"vscode-test": "^1.2.2",
		"webpack": "^5.0.0",
		"webpack-cli": "^4.2.0"
	},
	"dependencies": {
		"@glennsl/bs-json": "github:banacorn/bs-json#0c900d3",
		"agda-mode-vscode": "github:banacorn/agda-mode-vscode#v0.2.7",
		"bs-platform": "^8.3.3",
		"bs-vscode": "github:banacorn/bs-vscode#v0.0.16",
		"bs-webapi": ">=0.19.0",
		"eventemitter3": ">=4.0.0",
		"reason-nodejs": "^13.0.0-beta.1",
		"reason-promise": ">=1.1.0",
		"reason-react": "github:banacorn/reason-react#d1c98ce",
		"reason-react-update": "^2.0.0",
		"vscode-languageclient": "^6.1.3",
		"vscode-languageserver": "^6.1.1",
		"vscode-languageserver-textdocument": "^1.0.1",
		"yauzl": "^2.10.0"
	},
	"peerDependencies": {
		"react": "16.9.0",
		"react-dom": "16.9.0"
	},
	"enableProposedApi": false,
	"contributes": {
		"languages": [
			{
				"id": "guabao",
				"extensions": [
					".gcl"
				],
				"aliases": [
					"Guabao"
				],
				"configuration": "./language-configuration.json",
				"filenames": []
			}
		],
		"grammars": [
			{
				"language": "guabao",
				"scopeName": "source.gcl",
				"path": "./syntaxes/guabao.tmGrammar.json"
			}
		],
		"commands": [
			{
				"command": "guabao.refine",
				"title": "Refine",
				"category": "Guabao"
			},
			{
				"command": "guabao.restart",
				"title": "Restart",
				"category": "Guabao"
			},
			{
				"command": "guabao.debug",
				"title": "Debug",
				"category": "Guabao"
			}
		],
		"keybindings": [
			{
				"command": "guabao.refine",
				"key": "ctrl+c ctrl+r",
				"mac": "ctrl+c ctrl+r",
				"when": "resourceExtname == .gcl"
			},
			{
				"command": "guabao.restart",
				"key": "ctrl+x ctrl+r",
				"mac": "ctrl+x ctrl+r",
				"when": "resourceExtname == .gcl"
			}
		],
		"configuration": {
			"title": "Guabao",
			"properties": {
				"guabao.gclPath": {
					"type": "string",
					"default": "",
					"scope": "machine-overridable",
					"description": "Path to the GCL program."
				}
			}
		}
	},
	"resolutions": {
		"**/react": "16.9.0",
		"**/react-dom": "16.9.0"
	}
}
